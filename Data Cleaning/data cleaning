# Data Cleaning 
# Import Libraries
import pandas as pd
import numpy as np
# Loading Dataset
df = pd.read_csv(r"C:\Users\baira\OneDrive\Desktop\Technical Support Dataset.csv")
df
# Removing Duplicates
df = df.drop_duplicates()
# Selecting first 100 rows
df = df.head(100)
df
# Display dataset information
df.info()
# Drop unnecessary columns from the dataset
df = df.drop(columns=["Source"])
df = df.drop(columns=["First response time"])
df = df.drop(columns=["Expected SLA to resolve"])
df = df.drop(columns=["Expected SLA to first response"])
df = df.drop(columns=["SLA For Resolution"])
df = df.drop(columns=["Close time"])
df = df.drop(columns=["Survey results"])
df = df.drop(columns=["Latitude"])
df = df.drop(columns=["Longitude"])
df = df.drop(columns=["SLA For first response"])
df
# Arrange columns in Order
df= df[["Ticket ID","Agent Name","Topic","Priority","Country","Product group","Agent Group","Created time","Resolution time","Agent interactions","Support Level","Status"]]
df
# Changing Ticket ID values
df["Ticket ID"] = range(1,len(df)+1)
df
